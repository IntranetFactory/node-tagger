{
    "author"      : "Henri Bouvier",
    "name"        : "node-tagger",
    "description" : "Node Web Service to tag Part Of Speech (POS)",
    "version"     : "0.0.10",
    "repository"  : {
        "type" : "git",
        "url"  : "https://github.com/hbouvier/node-tagger.git"
    },
    "keywords": [
        "POS",
        "tagger",
        "Part Of Speech"
    ],
    "main" : "./lib/postagger.js",
    "bin"  : {
        "tagger" : "./bin/tagger"
    },
    "dependencies"         : {
        "express" : ">= 3.2.0 < 4.0.0",
        "winston" : "*",
        "pos"     : "0.1.2",
        "node-engine" : "latest",
        "node-options" : ">= 0.0.2",
        "node-network-monitor" : "latest"
    },
    "devDependencies"      : {
        
    },
    "optionalDependencies" : {
        
    },
    "engines"              : {
        "node" : ">= 0.8.0 < 0.11.0"
    },
    "scripts": {
        "test": "node test/test.js"
    },
    "configuration"        : {
        "name"    : "tagger",
        "level"   : "info",
        "workers" : 0,
        "script"  : "lib/main.js",
        "context" : "ws",
        "port"    : 3000
    }
}
